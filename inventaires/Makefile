compiler_odb_clean:
	-$(DEL_FILE) inventory-odb.cxx inventory-odb.hxx inventory-odb.ixx inventory.sql inventory_item-odb.cxx inventory_item-odb.hxx inventory_item-odb.ixx inventory_item.sql
inventory-odb.cxx: inventory.hxx \
		inventory.hxx \
		inventory_item.hxx
	odb -x -fPIC -x --std=c++11 --database pgsql --sql-name-case lower --profile qt --default-pointer QSharedPointer --generate-query --generate-session -I/usr/include/x86_64-linux-gnu/qt5 inventory.hxx

inventory_item-odb.cxx: inventory_item.hxx \
		inventory.hxx \
		inventory_item.hxx
	odb -x -fPIC -x --std=c++11 --database pgsql --sql-name-case lower --profile qt --default-pointer QSharedPointer --generate-query --generate-session -I/usr/include/x86_64-linux-gnu/qt5 inventory_item.hxx

compiler_odbh_make_all: inventory-odb.hxx inventory_item-odb.hxx
compiler_odbh_clean:
	-$(DEL_FILE) inventory-odb.hxx inventory_item-odb.hxx
inventory-odb.hxx: inventory.hxx \
		inventory-odb.cxx
	@true

inventory_item-odb.hxx: inventory_item.hxx \
		inventory_item-odb.cxx
	@true

compiler_rcc_make_all:
compiler_rcc_clean:
compiler_moc_header_make_all:
compiler_moc_header_clean:
compiler_moc_source_make_all:
compiler_moc_source_clean:
compiler_yacc_decl_make_all:
compiler_yacc_decl_clean:
compiler_yacc_impl_make_all:
compiler_yacc_impl_clean:
compiler_lex_make_all:
compiler_lex_clean:
compiler_clean: compiler_odb_clean compiler_odbh_clean 

####### Compile

inventory.o: inventory.cpp inventory.h \
		inventory_item.h \
		inventory-odb.hxx \
		inventory_item-odb.hxx \
		../produits/produit.h \
		../produits/types.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o inventory.o inventory.cpp

inventory_item.o: inventory_item.cpp inventory_item.h \
		inventory.h \
		inventory_item-odb.hxx \
		../produits/produit.h \
		../produits/types.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o inventory_item.o inventory_item.cpp

inventory-odb.o: inventory-odb.cxx 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o inventory-odb.o inventory-odb.cxx

inventory_item-odb.o: inventory_item-odb.cxx 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o inventory_item-odb.o inventory_item-odb.cxx

####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/usr/lib || mkdir -p $(INSTALL_ROOT)/usr/lib
	-$(INSTALL_PROGRAM) $(TARGET) $(INSTALL_ROOT)/usr/lib/$(TARGET)
	-$(STRIP) --strip-unneeded $(INSTALL_ROOT)/usr/lib/$(TARGET)
	-$(SYMLINK) $(TARGET) $(INSTALL_ROOT)/usr/lib/$(TARGET0)
	-$(SYMLINK) $(TARGET) $(INSTALL_ROOT)/usr/lib/$(TARGET1)
	-$(SYMLINK) $(TARGET) $(INSTALL_ROOT)/usr/lib/$(TARGET2)

uninstall_target: FORCE
	-$(DEL_FILE) $(INSTALL_ROOT)/usr/lib/$(TARGET) 
	 -$(DEL_FILE) $(INSTALL_ROOT)/usr/lib/$(TARGET0) 
	 -$(DEL_FILE) $(INSTALL_ROOT)/usr/lib/$(TARGET1) 
	 -$(DEL_FILE) $(INSTALL_ROOT)/usr/lib/$(TARGET2)
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/lib/ 


install: install_target  FORCE

uninstall: uninstall_target  FORCE

FORCE:

